JWT_ACCESS_SECRET = 
JWT_REFRESH_SECRET = 
JWT_EXPIRES = 1d

NODE_ENV=development


DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=12345678
DB_DATABASE=mes
DB_SCHEMA=public
DB_SSL=false
DB_POOL_SIZE=10
DB_POOL_MAX=20


# Cache 配置
CACHE_TTL=3600
CACHE_MAX_ITEMS=100
USE_REDIS=true

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=123456
REDIS_DB=0
REDIS_KEY_PREFIX=app:
INIT_PERMISSIONS=true


# 日志配置
LOG_LEVEL=debug
LOG_MAX_SIZE=20m
LOG_MAX_FILES=14d

# 告警配置
ALERT_ERROR_THRESHOLD=10
ALERT_ERROR_TIME_WINDOW=300000
ALERT_CHANNELS=email,slack






# APM 配置
APM_ENABLED=true
APM_SERVER_URL=http://localhost:8200

# Telemetry 配置
TELEMETRY_ENABLED=true
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# 告警渠道配置
SLACK_TOKEN=your-slack-token
SLACK_CHANNEL=alerts
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-password
SMTP_FROM=alerts@yourdomain.com
ALERT_EMAIL_TO=team@yourdomain.com

# 性能监控阈值
SLOW_RESPONSE_THRESHOLD=1000
ERROR_THRESHOLD=10